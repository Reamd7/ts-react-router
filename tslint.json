{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules":{
    "object-literal-sort-keys":false,
    "ordered-imports":false,
    "prefer-for-of":false,
    "no-empty":false,
    "no-shadowed-variable":false,
    "max-classes-per-file":false,
    "member-access":false,
    "no-console":false,
    "jsx-no-lambda":false
  }
}
